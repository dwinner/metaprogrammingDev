        {
            putValue( Env_TSA_DISP_Sign2_Passing, 0 );
        }
        putValue( Env_TSA_DISP_Sign2_General_Signs, 0 );
        putValue( Env_TSA_DISP_Sign2_NoPassing, 0 );
    }
    else if( ( Sign2_OnCAN == 0xCC ) )// "No entry"
    {
        putValue( Env_TSA_DISP_Sign2_General_Signs, 6 );
        putValue( Env_TSA_DISP_Sign2_NoPassing, 0 );
        putValue( Env_TSA_DISP_Sign2_Passing, 0 );
    }
    else if( ( Sign2_OnCAN == 0xCD ) )// "Pedestrian crossing"
    {
        if( ( Icon_Set_Type_OnCAN == cUS           ) ||
            ( Icon_Set_Type_OnCAN == cCanada       ) ||
            ( Icon_Set_Type_OnCAN == cSouthAmerica )    )
        {   // Speedlimit miles
