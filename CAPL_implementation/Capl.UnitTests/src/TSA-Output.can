/*@@caplFunc:Sign2_Eval():*///function
Sign2_Eval()
{
    // Evaluates Sign2_OnCAN and determines which icon has to be shown
    /* -------------------------- Speedlimit -------------------*/
    if( ( Sign2_OnCAN >= 0x01 ) && ( Sign2_OnCAN <=0x20 ) )
    {
        if( ( Icon_Set_Type_OnCAN == cUS     ) ||
            ( Icon_Set_Type_OnCAN == cCanada )    )
        {   // Speedlimit miles
            putValue( Env_TSA_DISP_Sign2_Speedlimit_numeric_Miles, Sign2_OnCAN );
            putValue( Env_TSA_DISP_Sign2_Speedlimit_numeric_KM, 0 ); // Clear KM-Signs
        }
        else
        {   // Speedlimit km/h
            putValue( Env_TSA_DISP_Sign2_Speedlimit_numeric_KM, Sign2_OnCAN );
            putValue( Env_TSA_DISP_Sign2_Speedlimit_numeric_Miles, 0 );
        }
    }
    else
    {
        putValue( Env_TSA_DISP_Sign2_Speedlimit_numeric_KM, 0 ); // Clear KM-Signs
        putValue( Env_TSA_DISP_Sign2_Speedlimit_numeric_Miles, 0 ); // Clear Miles-Signs
    }
    /* -------------------------- End of Speedlimit -------------------*/
    if( ( Sign2_OnCAN >= 0x21 ) && ( Sign2_OnCAN <=0x40 ) )
    {
        if( ( Icon_Set_Type_OnCAN == cUS     ) ||
            ( Icon_Set_Type_OnCAN == cCanada )    )
        {   // Speedlimit miles
            putValue( Env_TSA_DISP_Sign2_Endoflimit_numeric_Miles, Sign2_OnCAN - 0x20 );
            putValue( Env_TSA_DISP_Sign2_Endoflimit_numeric_KM, 0 ); // Clear KM-Signs
        }
        else
        {   // Speedlimit km/h
            putValue( Env_TSA_DISP_Sign2_Endoflimit_numeric_KM, Sign2_OnCAN - 0x20 );
            putValue( Env_TSA_DISP_Sign2_Endoflimit_numeric_Miles, 0 );
        }
    }
    else
    {
        putValue( Env_TSA_DISP_Sign2_Endoflimit_numeric_KM, 0 ); // Clear KM-Signs
        putValue( Env_TSA_DISP_Sign2_Endoflimit_numeric_Miles, 0 ); // Clear Miles-Signs
    }
    /* -------------------------- Minimum Speed -------------------*/
    if( ( Sign2_OnCAN >= 0x41 ) && ( Sign2_OnCAN <=0x60 ) )
    {
        if( ( Icon_Set_Type_OnCAN == cUS     ) ||
            ( Icon_Set_Type_OnCAN == cCanada )    )
        {   // Speedlimit miles
            putValue( Env_TSA_DISP_Sign2_MinSpeed_numeric_Miles, Sign2_OnCAN - 0x40 );
            putValue( Env_TSA_DISP_Sign2_MinSpeed_numeric_KM, 0 ); // Clear KM-Signs
            putValue( Env_TSA_DISP_Sign2_MinSpeed_numeric_Japan_Korea, 0 ); // Clear KM-Signs
            putValue( Env_TSA_DISP_Sign2_MinSpeed_numeric_China, 0 ); // Clear KM-Signs
        }
        else if( Icon_Set_Type_OnCAN == cChina )

        {   // Speedlimit km/h
            putValue( Env_TSA_DISP_Sign2_MinSpeed_numeric_China, Sign2_OnCAN - 0x40 );
            putValue( Env_TSA_DISP_Sign2_MinSpeed_numeric_KM, 0 ); // Clear KM-Signs
            putValue( Env_TSA_DISP_Sign2_MinSpeed_numeric_Miles, 0 ); // Clear KM-Signs
            putValue( Env_TSA_DISP_Sign2_MinSpeed_numeric_Japan_Korea, 0 ); // Clear KM-Signs
        }
        else if( ( Icon_Set_Type_OnCAN == cJapan     ) ||
                 ( Icon_Set_Type_OnCAN == cKorea )     )

        {   // Speedlimit km/h
            putValue( Env_TSA_DISP_Sign2_MinSpeed_numeric_Japan_Korea, Sign2_OnCAN - 0x40 );
            putValue( Env_TSA_DISP_Sign2_MinSpeed_numeric_KM, 0 ); // Clear KM-Signs
            putValue( Env_TSA_DISP_Sign2_MinSpeed_numeric_Miles, 0 ); // Clear KM-Signs
            putValue( Env_TSA_DISP_Sign2_MinSpeed_numeric_China, 0 ); // Clear KM-Signs
        }
        else 
        {   // Speedlimit km/h
            putValue( Env_TSA_DISP_Sign2_MinSpeed_numeric_KM, Sign2_OnCAN - 0x40 );
            putValue( Env_TSA_DISP_Sign2_MinSpeed_numeric_Japan_Korea, 0 ); // Clear KM-Signs
            putValue( Env_TSA_DISP_Sign2_MinSpeed_numeric_Miles, 0 ); // Clear KM-Signs
            putValue( Env_TSA_DISP_Sign2_MinSpeed_numeric_China, 0 ); // Clear KM-Signs
        }
    }
    else
    {
        putValue( Env_TSA_DISP_Sign2_MinSpeed_numeric_KM, 0 ); // Clear KM-Signs
        putValue( Env_TSA_DISP_Sign2_MinSpeed_numeric_Miles, 0 ); // Clear Miles-Signs
        putValue( Env_TSA_DISP_Sign2_MinSpeed_numeric_Japan_Korea, 0 ); // Clear KM-Signs
        putValue( Env_TSA_DISP_Sign2_MinSpeed_numeric_China, 0 ); // Clear KM-Signs
    }
    /* -------------------------- End of Minimum Speed -------------------*/
    if( ( Sign2_OnCAN >= 0x61 ) && ( Sign2_OnCAN <=0x80 ) )
    {
        if( ( Icon_Set_Type_OnCAN == cVienna_RHD   ) ||
            ( Icon_Set_Type_OnCAN == cVienna_LHD   ) ||
            ( Icon_Set_Type_OnCAN == cSouthAmerica ) ||
            ( Icon_Set_Type_OnCAN == cGulfStates   )    )
        {   // Speedlimit miles
            putValue( Env_TSA_DISP_Sign2_EndofMinSpeed_numeric_KM, Sign2_OnCAN - 0x60 );
        }
        else
        {   // Speedlimit km/h
            putValue( Env_TSA_DISP_Sign2_EndofMinSpeed_numeric_KM, 0 ); // Clear KM-Signs
        }
    }
    else
    {
        putValue( Env_TSA_DISP_Sign2_EndofMinSpeed_numeric_KM, 0 ); // Clear KM-Signs
    }
    /* -------------------------- Advisory Speed -------------------*/
    if( ( Sign2_OnCAN >= 0x81 ) && ( Sign2_OnCAN <=0xA0 ) )
    {
        if( ( Icon_Set_Type_OnCAN == cUS     )    )
        {   // Speedlimit miles
            putValue( Env_TSA_DISP_Sign2_AdvSpeed_numeric_US, Sign2_OnCAN - 0x80 );
            putValue( Env_TSA_DISP_Sign2_AdvSpeed_numeric_KM, 0 ); // Clear KM-Signs
            putValue( Env_TSA_DISP_Sign2_AdvSpeed_numeric_China_Canada, 0 ); // Clear KM-Signs
        }
        else if( ( Icon_Set_Type_OnCAN == cChina  ) ||
                 ( Icon_Set_Type_OnCAN == cCanada )     )

        {   // Speedlimit km/h
            putValue( Env_TSA_DISP_Sign2_AdvSpeed_numeric_China_Canada, Sign2_OnCAN - 0x80 );
            putValue( Env_TSA_DISP_Sign2_AdvSpeed_numeric_KM, 0 ); // Clear KM-Signs
            putValue( Env_TSA_DISP_Sign2_AdvSpeed_numeric_US, 0 ); // Clear KM-Signs
        }
        else if( ( Icon_Set_Type_OnCAN == cVienna_RHD ) ||
                 ( Icon_Set_Type_OnCAN == cVienna_LHD )     )

        {   // Speedlimit km/h
            putValue( Env_TSA_DISP_Sign2_AdvSpeed_numeric_KM, Sign2_OnCAN - 0x80 );
            putValue( Env_TSA_DISP_Sign2_AdvSpeed_numeric_China_Canada, 0 ); // Clear KM-Signs
            putValue( Env_TSA_DISP_Sign2_AdvSpeed_numeric_US, 0 ); // Clear KM-Signs
        }
        else 
        {   // Speedlimit km/h
            putValue( Env_TSA_DISP_Sign2_AdvSpeed_numeric_KM, 0 );
            putValue( Env_TSA_DISP_Sign2_AdvSpeed_numeric_China_Canada, 0 ); // Clear KM-Signs
            putValue( Env_TSA_DISP_Sign2_AdvSpeed_numeric_US, 0 ); // Clear KM-Signs
        }
    }
    else
    {
        putValue( Env_TSA_DISP_Sign2_AdvSpeed_numeric_KM, 0 );
        putValue( Env_TSA_DISP_Sign2_AdvSpeed_numeric_China_Canada, 0 ); // Clear KM-Signs
        putValue( Env_TSA_DISP_Sign2_AdvSpeed_numeric_US, 0 ); // Clear KM-Signs
    }
    /* -------------------------- End of Advisory Speed -------------------*/
    if( ( Sign2_OnCAN >= 0xA1 ) && ( Sign2_OnCAN <=0xC0 ) )
    {
        if( ( Icon_Set_Type_OnCAN == cVienna_RHD   ) ||
            ( Icon_Set_Type_OnCAN == cVienna_LHD   )    )
        {   // Speedlimit miles
            putValue( Env_TSA_DISP_Sign2_EndofAdvSpeed_numeric_KM, Sign2_OnCAN - 0xA0 );
        }
        else
        {   // Speedlimit km/h
            putValue( Env_TSA_DISP_Sign2_EndofAdvSpeed_numeric_KM, 0 ); // Clear KM-Signs
        }
    }
    else
    {
        putValue( Env_TSA_DISP_Sign2_EndofAdvSpeed_numeric_KM, 0 ); // Clear KM-Signs
    }
    /* -------------------------- General Signs -------------------*/
    if( ( Sign2_OnCAN < 0xC1 ) )// "End of Limit km/h"
    {
        putValue( Env_TSA_DISP_Sign2_General_Signs, 0 );
        putValue( Env_TSA_DISP_Sign2_NoPassing, 0 );
        putValue( Env_TSA_DISP_Sign2_Passing, 0 );
    }
    if( ( Sign2_OnCAN == 0xC1 ) )// "End of Limit km/h"
    {
        if( ( Icon_Set_Type_OnCAN == cVienna_RHD   ) ||
            ( Icon_Set_Type_OnCAN == cVienna_LHD   ) ||
            ( Icon_Set_Type_OnCAN == cChina        ) ||
            ( Icon_Set_Type_OnCAN == cKorea        ) ||
            ( Icon_Set_Type_OnCAN == cJapan        ) ||
            ( Icon_Set_Type_OnCAN == cSouthAmerica ) ||
            ( Icon_Set_Type_OnCAN == cGulfStates   )    )
        {   // Speedlimit miles
            putValue( Env_TSA_DISP_Sign2_General_Signs, 1 );
        }
        else
        {
            putValue( Env_TSA_DISP_Sign2_General_Signs, 0 );
            putValue( Env_TSA_DISP_Sign2_NoPassing, 0 );
            putValue( Env_TSA_DISP_Sign2_Passing, 0 );
        }
    }
    else if( ( Sign2_OnCAN == 0xC2 ) )// "End of Limit miles"
    {
        if( ( Icon_Set_Type_OnCAN == cVienna_RHD   ) ||
            ( Icon_Set_Type_OnCAN == cVienna_LHD   ) ||
            ( Icon_Set_Type_OnCAN == cChina        ) ||
            ( Icon_Set_Type_OnCAN == cKorea        ) ||
            ( Icon_Set_Type_OnCAN == cJapan        ) ||
            ( Icon_Set_Type_OnCAN == cSouthAmerica ) ||
            ( Icon_Set_Type_OnCAN == cGulfStates   )    )
        {   // Speedlimit miles
            putValue( Env_TSA_DISP_Sign2_General_Signs, 2 );
        }
        else
        {
            putValue( Env_TSA_DISP_Sign2_General_Signs, 0 );
            putValue( Env_TSA_DISP_Sign2_NoPassing, 0 );
            putValue( Env_TSA_DISP_Sign2_Passing, 0 );
        }
    }
    else if( ( Sign2_OnCAN == 0xC3 ) )// "End of restrictions"
    {
        if( ( Icon_Set_Type_OnCAN == cVienna_RHD   ) ||
            ( Icon_Set_Type_OnCAN == cVienna_LHD   ) ||
            ( Icon_Set_Type_OnCAN == cChina        ) ||
            ( Icon_Set_Type_OnCAN == cKorea        ) ||
            ( Icon_Set_Type_OnCAN == cJapan        ) ||
            ( Icon_Set_Type_OnCAN == cSouthAmerica ) ||
            ( Icon_Set_Type_OnCAN == cGulfStates   )    )
        {   // Speedlimit miles
            putValue( Env_TSA_DISP_Sign2_General_Signs, 3 );
        }
        else
        {
            putValue( Env_TSA_DISP_Sign2_General_Signs, 0 );
            putValue( Env_TSA_DISP_Sign2_NoPassing, 0 );
            putValue( Env_TSA_DISP_Sign2_Passing, 0 );
        }
    }
    else if( ( Sign2_OnCAN == 0xC4 ) )// "Residential area"
    {
        if( ( Icon_Set_Type_OnCAN == cVienna_RHD   ) ||
            ( Icon_Set_Type_OnCAN == cVienna_LHD   )    )
        {   // Speedlimit miles
            putValue( Env_TSA_DISP_Sign2_General_Signs, 4 );
        }
        else
        {
            putValue( Env_TSA_DISP_Sign2_General_Signs, 0 );
            putValue( Env_TSA_DISP_Sign2_NoPassing, 0 );
            putValue( Env_TSA_DISP_Sign2_Passing, 0 );
        }
    }
    else if( ( Sign2_OnCAN == 0xC5 ) )// "end of residential area"
    {
        if( ( Icon_Set_Type_OnCAN == cVienna_RHD   ) ||
            ( Icon_Set_Type_OnCAN == cVienna_LHD   )    )
        {   // Speedlimit miles
            putValue( Env_TSA_DISP_Sign2_General_Signs, 5 );
        }
        else
        {
            putValue( Env_TSA_DISP_Sign2_General_Signs, 0 );
            putValue( Env_TSA_DISP_Sign2_NoPassing, 0 );
            putValue( Env_TSA_DISP_Sign2_Passing, 0 );
        }
    }
    else if( ( Sign2_OnCAN >= 0xC6 ) && ( Sign2_OnCAN <= 0xC9 ) )// "Empty" including no passing
    {
        putValue( Env_TSA_DISP_Sign2_General_Signs, 0 );
    }
    else if( Sign2_OnCAN == 0xCA )// "No Passing"
    {
        if( ( Icon_Set_Type_OnCAN == cVienna_RHD   )    )
        {   // Speedlimit miles
            putValue( Env_TSA_DISP_Sign2_NoPassing, 1 );
        }
        else if( ( Icon_Set_Type_OnCAN == cVienna_LHD   )    )
        {   // Speedlimit miles
            putValue( Env_TSA_DISP_Sign2_NoPassing, 2 );
        }
        else if( ( Icon_Set_Type_OnCAN == cJapan   )    )
        {   // Speedlimit miles
            putValue( Env_TSA_DISP_Sign2_NoPassing, 3 );
        }
        else if( ( Icon_Set_Type_OnCAN == cChina   )    )
        {   // Speedlimit miles
            putValue( Env_TSA_DISP_Sign2_NoPassing, 4 );
        }
        else if( ( Icon_Set_Type_OnCAN == cKorea   )    )
        {   // Speedlimit miles
            putValue( Env_TSA_DISP_Sign2_NoPassing, 5 );
        }
        else if( ( Icon_Set_Type_OnCAN == cUS   )    )
        {   // Speedlimit miles
            putValue( Env_TSA_DISP_Sign2_NoPassing, 6 );
        }
        else if( ( Icon_Set_Type_OnCAN == cCanada   )    )
        {   // Speedlimit miles
            putValue( Env_TSA_DISP_Sign2_NoPassing, 7 );
        }
        else if( ( Icon_Set_Type_OnCAN == cSouthAmerica   )    )
        {   // Speedlimit miles
            putValue( Env_TSA_DISP_Sign2_NoPassing, 8 );
        }
        else if( ( Icon_Set_Type_OnCAN == cGulfStates   )    )
        {   // Speedlimit miles
            putValue( Env_TSA_DISP_Sign2_NoPassing, 9 );
        }
        else
        {
            putValue( Env_TSA_DISP_Sign2_NoPassing, 0 );
        }
        putValue( Env_TSA_DISP_Sign2_General_Signs, 0 );
        putValue( Env_TSA_DISP_Sign2_Passing, 0 );
    }
    else if( Sign2_OnCAN == 0xCB )// "End of No Passing"
    {
        if( ( Icon_Set_Type_OnCAN == cVienna_RHD   )    )
        {   // Speedlimit miles
            putValue( Env_TSA_DISP_Sign2_Passing, 1 );
        }
        else if( ( Icon_Set_Type_OnCAN == cVienna_LHD   )    )
        {   // Speedlimit miles
            putValue( Env_TSA_DISP_Sign2_Passing, 2 );
        }
        else if( ( Icon_Set_Type_OnCAN == cJapan   )    )
        {   // Speedlimit miles
            putValue( Env_TSA_DISP_Sign2_Passing, 3 );
        }
        else if( ( Icon_Set_Type_OnCAN == cChina   )    )
        {   // Speedlimit miles
            putValue( Env_TSA_DISP_Sign2_Passing, 4 );
        }
        else if( ( Icon_Set_Type_OnCAN == cKorea   )    )
        {   // Speedlimit miles
            putValue( Env_TSA_DISP_Sign2_Passing, 5 );
        }
        else if( ( Icon_Set_Type_OnCAN == cUS   )    )
        {   // Speedlimit miles
            putValue( Env_TSA_DISP_Sign2_Passing, 6 );
        }
        else if( ( Icon_Set_Type_OnCAN == cCanada   )    )
        {   // Speedlimit miles
            putValue( Env_TSA_DISP_Sign2_Passing, 7 );
        }
