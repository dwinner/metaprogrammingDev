            putValue( Env_TSA_DISP_Sign2_General_Signs, 10 );
            putValue( Env_TSA_DISP_Sign2_NoPassing, 0 );
            putValue( Env_TSA_DISP_Sign2_Passing, 0 );
        }
        else
        {
            putValue( Env_TSA_DISP_Sign2_General_Signs, 9 );
            putValue( Env_TSA_DISP_Sign2_NoPassing, 0 );
            putValue( Env_TSA_DISP_Sign2_Passing, 0 );
        }
    }
    else if( ( Sign2_OnCAN == 0xFF ) )// "SNA"
    {
        putValue( Env_TSA_DISP_Sign2_General_Signs, 0 );
        putValue( Env_TSA_DISP_Sign2_NoPassing, 0 );
        putValue( Env_TSA_DISP_Sign2_Passing, 0 );
    }
    else
    {
        putValue( Env_TSA_DISP_Sign2_General_Signs, 0 );
        putValue( Env_TSA_DISP_Sign2_NoPassing, 0 );
        putValue( Env_TSA_DISP_Sign2_Passing, 0 );
    }
}
