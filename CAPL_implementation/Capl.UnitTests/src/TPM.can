{   /* Botschaft Tire_Temp_AR ID: 1031 --- Sendeart 0 cyclicX --- 
BA_ "ApplMessage" BO_ 1031 0;
BA_ "NmAsrMessage" BO_ 1031 0;
BA_ "GenMsgMinAcceptLength" BO_ 1031 8;
BA_ "GenMsgILSupport" BO_ 1031 1;
BA_ "DiagState" BO_ 1031 0;
BA_ "DiagRequest" BO_ 1031 0;
BA_ "DiagResponse" BO_ 1031 0;
BA_ "GenMsgStartValue" BO_ 1031 "0";
BA_ "GenMsgStartDelayTime" BO_ 1031 290;
BA_ "GenMsgCycleTime" BO_ 1031 1000;
BA_ "GenMsgSendType" BO_ 1031 0;
    */
    _Tire_Temp_AR_Timebase =          c_tDBKomTimer_20ms;
    _Tire_Temp_AR_CycleCount =      1000 / _Tire_Temp_AR_Timebase;
    _Tire_Temp_AR_StartDelay =       290 / _Tire_Temp_AR_Timebase;
    _Tire_Temp_AR_Cnt = _Tire_Temp_AR_StartDelay;

    putValue( HMI__STMSGSD_Tire_Temp_AR, _Tire_Temp_AR_StartDelay * _Tire_Temp_AR_Timebase );

    _mTire_Temp_AR.RefPress_Front.phys = getValue( HMI_RefPress_Front );
    _mTire_Temp_AR.RefPress_Rear.phys = getValue( HMI_RefPress_Rear );
    _mTire_Temp_AR.Tire_Temp_Stat_FL.phys = getValue( HMI_Tire_Temp_Stat_FL );
    _mTire_Temp_AR.Tire_Temp_Stat_FR.phys = getValue( HMI_Tire_Temp_Stat_FR );
    _mTire_Temp_AR.Tire_Temp_Stat_RL.phys = getValue( HMI_Tire_Temp_Stat_RL );
    _mTire_Temp_AR.Tire_Temp_Stat_RR.phys = getValue( HMI_Tire_Temp_Stat_RR );
    _mTire_Temp_AR.Tire_Temp_WarnDisp_Rq.phys = getValue( HMI_Tire_Temp_WarnDisp_Rq );
    _mTire_Temp_AR.TireTemp_FL.phys = getValue( HMI_TireTemp_FL );
    _mTire_Temp_AR.TireTemp_FR.phys = getValue( HMI_TireTemp_FR );
    _mTire_Temp_AR.TireTemp_RL.phys = getValue( HMI_TireTemp_RL );
    _mTire_Temp_AR.TireTemp_RR.phys = getValue( HMI_TireTemp_RR );
}
