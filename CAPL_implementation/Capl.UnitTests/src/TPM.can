/*@@caplFunc:ROE_TPM_check_output():*/
void ROE_TPM_check_output()
{
    if (!getValue(HMI__STMSG_ROE_TPM ))
    {
        // Botschaft ROE_TPM --- Sendeart 9 cyclicWithRepeatOnDemand --- 
        if( _ROE_TPM_Cnt > 0 )
        {
            _ROE_TPM_Cnt--;
            if( _ROE_TPM_Cnt < 1 )
            {
                // Nachricht wg. abgelaufenem Zyklus ausgeben
                output( _mROE_TPM );
                _ROE_TPM_MsgRepetitions--;
                if( _ROE_TPM_MsgRepetitions > 0 ) _ROE_TPM_Cnt = _ROE_TPM_CycleCount;
            }
        }
    }
}
