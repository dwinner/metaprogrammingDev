/*@@caplFunc:Tire_State2_AR_output():*/
Tire_State2_AR_output()
{
    byte crc;
    byte SQC_help;

    SQC_help = getValue( HMI_SQC_TPM_Lmp_On_Rq ); //get copy of actual EnvVar
    if( !getValue( HMI_SQC_TPM_Lmp_On_Rq_Auto ) ) // automatic SQC-calculation active ?
    {
        if( SQC_help == _Tire_State2_AR_SQC_TriggPos ) // start injection of error when trigger reached
        {
            _Tire_State2_AR_E2E_Armed = 1;   // run
            putValue( HMI_E2E_TPM_Lmp_On_Rq_State, 1 );
        }
        // calculate new sequence counter
        
        if( ( _Tire_State2_AR_SQC_WrongSeqJump != 0 ) &&
            ( _Tire_State2_AR_E2E_Armed == 1        )    )
        {
            SQC_help = SQC_help + 16 + _Tire_State2_AR_SQC_WrongSeqJump;
            if( SQC_help > 15 )
                SQC_help = SQC_help - 16;
            if( SQC_help > 15 )
                SQC_help = SQC_help - 16;
            _Tire_State2_AR_SQC_WrongSeqJump = 0;
        }
        
        if( ( _Tire_State2_AR_SQC_RepeatedCnt ) &&
            ( _Tire_State2_AR_E2E_Armed == 1  )    )
        {
            _Tire_State2_AR_SQC_RepeatedCnt--;
        }
        else
        {
            if( SQC_help >= 15 ) SQC_help = 0;
            else SQC_help++;
        }
            
        putValue( HMI_SQC_TPM_Lmp_On_Rq, SQC_help );
    }
    _mTire_State2_AR.SQC_TPM_Lmp_On_Rq = SQC_help;

    if( getValue( HMI_CRC_TPM_Lmp_On_Rq_Profile_02 ) )
    {
        if( !getValue( HMI_CRC_TPM_Lmp_On_Rq_Auto ) )
        {   // CRC according Profile_02 ( W213 )
            // berechne crc
            crc = 0xFF;
//          crc = crctable_profile02[ crc ^ _mTire_State2_AR.byte( 0 ) ];  CRC
            crc = crctable_profile02[ crc ^ _mTire_State2_AR.byte( 1 ) ];
//            crc = crctable_profile02[ crc ^ _mTire_State2_AR.byte( 2 ) ];
//            crc = crctable_profile02[ crc ^ _mTire_State2_AR.byte( 3 ) ];
//            crc = crctable_profile02[ crc ^ _mTire_State2_AR.byte( 4 ) ];
//            crc = crctable_profile02[ crc ^ _mTire_State2_AR.byte( 5 ) ];
//            crc = crctable_profile02[ crc ^ _mTire_State2_AR.byte( 6 ) ];
//            crc = crctable_profile02[ crc ^ _mTire_State2_AR.byte( 7 ) ];
            crc = crctable_profile02[ crc ^ _Tire_State2_AR_CRC_Offset[ _mTire_State2_AR.SQC_TPM_Lmp_On_Rq ] ]; // load offset
            crc = (crc ^ 0xFF);
            
            if( ( _Tire_State2_AR_CRC_ErrorCnt   && ( _Tire_State2_AR_E2E_Armed == 1   ) ) ||
                ( getValue( HMI_CRC_TPM_Lmp_On_Rq_Invalid )                                         )    )
            {
                crc++;
                if( _Tire_State2_AR_CRC_ErrorCnt > 0 )
                    _Tire_State2_AR_CRC_ErrorCnt--; 
            }
            _mTire_State2_AR.CRC_TPM_Lmp_On_Rq = crc;
            putValue( HMI_CRC_TPM_Lmp_On_Rq, crc );
        }
        else
            _mTire_State2_AR.CRC_TPM_Lmp_On_Rq = getValue( HMI_CRC_TPM_Lmp_On_Rq );
    }
    if( ( _Tire_State2_AR_MSG_DropCnt > 0 ) &&
        ( _Tire_State2_AR_E2E_Armed == 1  )    )
    {
        _Tire_State2_AR_MSG_DropCnt--;  // do not send message
    }
    else
        output( _mTire_State2_AR );
        
    if( _Tire_State2_AR_E2E_Armed == 1 )
    {
        if ( ( _Tire_State2_AR_MSG_DropCnt == 0      ) &&
             ( _Tire_State2_AR_CRC_ErrorCnt == 0     ) &&
             ( _Tire_State2_AR_SQC_RepeatedCnt == 0  ) &&
             ( _Tire_State2_AR_SQC_WrongSeqJump == 0 )    )
        {
            _Tire_State2_AR_E2E_Armed = 0; // no further influence, injetion completed
            putValue( HMI_E2E_TPM_Lmp_On_Rq_State, 0 );
        }
    }
}
/*@@end*/

