/*@@caplFunc:Tire_State2_AR_check_output():*/
Tire_State2_AR_check_output()
{
    if (!getValue(HMI__STMSG_Tire_State2_AR ))
    {
        // Botschaft Tire_State2_AR ID: 1036 --- Sendeart 5 cyclicAndSpontanWithDelay --- 
        _Tire_State2_AR_Cnt--;
        if( _Tire_State2_AR_Cnt < 1 )
        {
            // Nachricht wg. abgelaufenem Zyklus ausgeben
            Tire_State2_AR_output();
            _Tire_State2_AR_Cnt = _Tire_State2_AR_CycleCount; // Zyklus-Zähler zurücksetzen
            _Tire_State2_AR_SpontanFlag = 0; // Spontan-Flag zurücksetzen
        }
        else if( _Tire_State2_AR_SpontanFlag)
        {
            // Nachricht wg. spontaner Änderung ausgeben (Mindestsendeabstand eingehalten)
            Tire_State2_AR_output();
            _Tire_State2_AR_SpontanFlag = 0; // Spontan-Flag zurücksetzen
        }
    }
}
/*@@end*/

