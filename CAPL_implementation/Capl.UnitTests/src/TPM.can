/*@@startStart:Start:*/
on start
{
    putValue( HMI__NM_TPM_NM_Request, 0 );  // reset all requests
    _NM_Switch_NM_Mode();
    
    // DBKom-Task starten
    
    __TPM_Init_Msg();
    __TPM_Startup_Delay();
   
    putValue( HMI__NM_TPM_AutomaticModeActive, 0 );
    putValue( HMI__NM_TPM_AutomaticModeActive, 1 );
}
/*@@end*/

/*@@caplFunc:__TPM_Init_Msg():*///function
__TPM_Init_Msg()
{
// #START_INIT#
    ROE_TPM_init();
    Tire_State_AR_init();
    Tire_State2_AR_init();
    Tire_Temp_AR_init();
// #START_INIT_END#
//    Meas_TPM_1_init();
    NM_xxx_AR_init();
//    XCP_Rs_TPM_init();
}
/*@@end*/

/*@@envVar:HMI__STOP_All_MESSAGES_DEFAULT_STARTDELAY:*/
on envVar HMI__STOP_All_MESSAGES_DEFAULT_STARTDELAY
{
    if( getValue( this ) )
    {
        __TPM_Init_Msg();
        __TPM_Startup_Delay();
    }
}
/*@@end*/

/*@@envVar:HMI__STOP_All_MESSAGES_SET_STARTDELAY:*/
on envVar HMI__STOP_All_MESSAGES_SET_STARTDELAY
{
    if( getValue( this ) )
    {
        __TPM_Startup_Delay();
    }
}
/*@@end*/

/*@@caplFunc:__TPM_Startup_Delay():*/
__TPM_Startup_Delay()
{
//    write(" Delay setzen :__TPM_Startup_Delay");

// #START_DELAY#
    _ROE_TPM_Cnt =                getValue( HMI__STMSGSD_ROE_TPM           ) / _ROE_TPM_Timebase;
    _Tire_State_AR_Cnt =          getValue( HMI__STMSGSD_Tire_State_AR     ) / _Tire_State_AR_Timebase;
    _Tire_State2_AR_Cnt =         getValue( HMI__STMSGSD_Tire_State2_AR    ) / _Tire_State2_AR_Timebase;
    _Tire_Temp_AR_Cnt =           getValue( HMI__STMSGSD_Tire_Temp_AR      ) / _Tire_Temp_AR_Timebase;
// #START_DELAY_END#
}
/*@@end*/

/*@@timer:t_DBKomTimer_5ms:*/
on Timer t_DBKomTimer_5ms
{
// *** DBKom-Sendefunktion ***
    // timer aufziehen für nächsten Zyklus
    if( getValue( HMI__NM_TPM_CommActive    ) |
        getValue( HMI__NM_TPM_RequestBusCom )   )
    {
        setTimer( t_DBKomTimer_5ms, c_tDBKomTimer_5ms );
    }
// #CYC_TIMER_5_MS#
//    CLkS_Rq_AR_check_output();                   // Botschaft CLkS_Rq_AR                   --- Sendeart 8 cyclicIfActiveFast --- 
// #CYC_TIMER_5_MS_END#
}
/*@@end*/

/*@@timer:t_DBKomTimer_20ms:*/
on Timer t_DBKomTimer_20ms
{
// *** DBKom-Sendefunktion ***
    // timer aufziehen für nächsten Zyklus
    if( getValue( HMI__NM_TPM_CommActive    ) |
        getValue( HMI__NM_TPM_RequestBusCom )   )
    {
        setTimer( t_DBKomTimer_20ms, c_tDBKomTimer_20ms );
    }
// #CYC_TIMER_20_MS#
    ROE_TPM_check_output();
    Tire_State_AR_check_output();
    Tire_State2_AR_check_output();
    Tire_Temp_AR_check_output();
// #CYC_TIMER_20_MS_END#

//    Meas_TPM_1_check_output();
//    NM_TPM_AR_check_output();
//    XCP_Rs_TPM_check_output();
}
