/*@@envVar:HMI__SAVE_PANEL_CONFIG_TPM:*/
on EnvVar HMI__SAVE_PANEL_CONFIG_TPM
{
    dword hFile;

    if( getValue(this) )
    {
        write("Saving HMI Panel Variables ...");

        hFile = openFileWrite( "CONFIG\\HMI_PRESETS.ASC", 0 );
        filePutString( "base hex timestamps absolute\n", 31, hFile );

// #MSG_SAVEENVVAR#
//        ROE_TPM_saveEnvVars( hFile );   
        Tire_State_AR_saveEnvVars( hFile );   
        Tire_Temp_AR_saveEnvVars( hFile );   
// #MSG_SAVEENVVAR_END#
//        Meas_TPM_1_saveEnvVars( hFile );   
        NM_xxx_AR_saveEnvVars( hFile );
//        XCP_Rs_TPM_saveEnvVars( hFile );   

        fileClose(hFile );
    }
}
/*@@end*/

