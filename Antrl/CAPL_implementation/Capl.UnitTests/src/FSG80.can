    else head[0] = head[0] & 0xEF;
    // Direction Bit in Byte 0 Bit 5 kopieren
    if( getValue(A_FSG_80_0_0_24_Changed_Direction) > 0) head[0] = head[0] | 0x20;
    else head[0] = head[0] & 0xDF;
    // TransmissionPos Bit in Byte 0 Bit 6 kopieren
    if( getValue(A_FSG_80_0_0_24_Changed_TransPos) > 0) head[0] = head[0] | 0x40;
    else head[0] = head[0] & 0xBF;
    if( getValue(A_FSG_80_0_0_24_Changed_IndexSize) > 0)
    {
        // 5 Byte Header
        head[0] = head[0] | 0x80;
        head[1] =  getValue(A_FSG_80_0_0_24_Changed_Start) & 0xFF;
        head[2] = (getValue(A_FSG_80_0_0_24_Changed_Start) >> 8 ) & 0xFF;
        head[3] =  getValue(A_FSG_80_0_0_24_Changed_Elements) & 0xFF;
        head[4] = (getValue(A_FSG_80_0_0_24_Changed_Elements)  >> 8 ) & 0xFF;
        //ToDo: DATA - derzeit einfach laenge = 5
        putValue(A_FSG_80_0_0_24_Changed, head, 5); 
    }
    else 
    {
        // 3 Byte Header
        head[0] = head[0] & 0x7F;
        head[1] = getValue(A_FSG_80_0_0_24_Changed_Start) & 0xFF;
        head[2] = getValue(A_FSG_80_0_0_24_Changed_Elements) & 0xFF;
        //ToDo: DATA - derzeit einfach laenge = 3
        putValue(A_FSG_80_0_0_24_Changed, head, 3); 
    }
}

