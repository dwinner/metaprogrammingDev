on envVar A_FSG_80_0_0_24Changed_BTN
{
    byte data[1212]; 
    byte stringValue[1212];
    byte head[5];
    int i;
    int headLength;
    long tempValue;
    word currentPos;
    word startVal;
    word elementsVal;
  byte nibbleSetted;
    if (A_FSG_80_0_0_24Changed_BTN_already_pressed == 0)
    {
        putValue(A_FSG_80_0_0_24_ErrCode,0);
        SetControlBackColor(panel_name,"errField_A_FSG_80_0_0_24",MakeRGB(255,255,255));
        A_FSG_80_0_0_24Changed_BTN_already_pressed = 1;
    }
    else 
    {
        currentPos = 0;
        //head Parameter
        headLength = getValueSize(A_FSG_80_0_0_24_Changed);
        getValue(A_FSG_80_0_0_24_Changed,head);
        for(i=0; i< headLength; i++)
        {
            data[currentPos++] = head[i];
        }
    //data Parameter
     startVal = getValue(A_FSG_80_0_0_24_Changed_Start);
     elementsVal = getValue(A_FSG_80_0_0_24_Changed_Elements);
     nibbleSetted = 0;
    if( (head[0]&0x20) == 0)
    {
     if( (head[0] & 0x10) > 0)
     {
        startVal++;
     }
     for( i = startVal ; i < startVal + elementsVal ; i++)
     {
       if( i < 0 || i >= A_FSG_80_0_0_24_ArrayDataList_Length )
       {
         write("Array index ausserhalb der Grenzen, Fct: 24");
         break;
       }
      if(nibbleSetted == 1) currentPos++;
      nibbleSetted = 0;
    //sollte der letzte Parameter ein first nibble gewesen sein, muss noch 1 increment durchgefuehrt werden
    if( nibbleSetted==1) currentPos++;
    nibbleSetted = 0;
     }
    }
    else
    {
     if( (head[0] & 0x10) > 0)
     {
        if(startVal != 0) startVal--;
        else
        {
           startVal = A_FSG_80_0_0_24_ArrayDataList_Length -1;
        }
     }
     for( i = startVal ; i > startVal - elementsVal ; i--)
     {
       if( i < 0 || i >= A_FSG_80_0_0_24_ArrayDataList_Length )
       {
         write("Array index ausserhalb der Grenzen, Fct: 24");
         break;
       }
      if(nibbleSetted == 1) currentPos++;
      nibbleSetted = 0;
    //sollte der letzte Parameter ein first nibble gewesen sein, muss noch 1 increment durchgefuehrt werden
    if( nibbleSetted==1) currentPos++;
    nibbleSetted = 0;
     }
    }

       Request_ByteSequence(LSGID, A_FSG_80_0_0_24_ID, Changed_REQ, data, currentPos);
       if(getValue(A_FSG_80_0_0_24_IntTlgEn)) //getValue(IT))
       {
           intertelegram_err(A_FSG_80_0_0_24_ID);
       }
       if(getValue(A_FSG_80_0_0_24_SegmEn))   //getvalue(SG)
       {
             sequence_err(A_FSG_80_0_0_24_ID,getValueSize(A_FSG_80_0_0_24_Changed));
       }
        A_FSG_80_0_0_24Changed_BTN_already_pressed = 0;
    }
