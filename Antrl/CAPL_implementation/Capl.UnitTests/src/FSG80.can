			return;
		}
	}
	currentPos += cnt4+1;
	for(i = 0; i < len4; i++)
	{
		superval[currentPos++]= param4[i];
	}

// gen_BuildFunc_SetSuperVals 
	@P_FSG_80_0_0_26_5_range = round(@P_FSG_80_0_0_26_5 + (0), 1.0); // slider value + min = real value
