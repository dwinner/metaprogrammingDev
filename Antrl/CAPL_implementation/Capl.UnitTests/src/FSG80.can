           intertelegram_err(P_FSG_80_0_0_14_ID);
        }
        if(getValue(P_FSG_80_0_0_14_SegmEn))   //getvalue(SG)
        {
           sequence_err(P_FSG_80_0_0_14_ID,elCount(BAPTemp));
        }
       P_FSG_80_0_0_14Status_BTN_already_pressed = 0;
    }
}
