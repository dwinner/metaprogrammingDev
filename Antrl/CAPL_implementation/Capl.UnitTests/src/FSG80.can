on envVar P_FSG_80_0_0_14 // bus value of FSG_Setup (14).
{
	// opcodes: Error-Status
	byte param1; // AvailableToneSettings
	byte param2; // AvailableSoundSettings
	byte param3; // maxVolume
	byte param4; // maxRadioFavorites
	byte param5; // StepsTreble
	byte param6; // StepsBass
	byte param7; // StepsBalance
	byte param8; // Extension1
	byte param9; // StepsGALA
	byte param10[61]; // BTNodeName
	word len10;
	byte cnt10;
	byte param11; // Extension2
	byte param12; // Extension3
	// DATATYPE_BYTESEQUENCE / DATATYPE_FIXEDBYTESEQUENCE
	int i;
	byte superval[71];
	int currentPos;
	currentPos = 0;
	for( i = 0; i < 71; i++ ) {
		superval[i] = 0;
	}
	getValue(P_FSG_80_0_0_14, superval);
	///////// Start param1 - UINT8 /////////
	param1 = superval[currentPos++];
	putValue(P_FSG_80_0_0_14_1, param1);
	///////// Start param2 - UINT8 /////////
	param2 = superval[currentPos++];
	putValue(P_FSG_80_0_0_14_2, param2);
	///////// Start param3 - UINT8 /////////
	param3 = superval[currentPos++];
	putValue(P_FSG_80_0_0_14_3, round((param3 * 1.0) - (0) + (0), 1.0));
	///////// Start param4 - UINT8 /////////
	param4 = superval[currentPos++];
	putValue(P_FSG_80_0_0_14_4, round((param4 * 1.0) - (0) + (0), 1.0));
	///////// Start param5 - UINT8 /////////
	param5 = superval[currentPos++];
	putValue(P_FSG_80_0_0_14_5, round((param5 * 1.0) - (0) + (0), 1.0));
	///////// Start param6 - UINT8 /////////
	param6 = superval[currentPos++];
	putValue(P_FSG_80_0_0_14_6, round((param6 * 1.0) - (0) + (0), 1.0));
	///////// Start param7 - UINT8 /////////
	param7 = superval[currentPos++];
	putValue(P_FSG_80_0_0_14_7, round((param7 * 1.0) - (0) + (0), 1.0));
	///////// Start param8 - NIBBLE /////////
	param8 = (superval[currentPos] & 0xf0)>>4;
	putValue(P_FSG_80_0_0_14_8, round((param8 * 1.0) - (0) + (0), 1.0));
	///////// Start param9 - NIBBLE /////////
	param9 = superval[currentPos++] & 0x0f;
	putValue(P_FSG_80_0_0_14_9, round((param9 * 1.0) - (0) + (0), 1.0));
	///////// Start param10 - STRING /////////
	getValue(P_FSG_80_0_0_14_10, param10);
	if((superval[currentPos] & 0x80) > 0)
	{
		if(60>127) /* Max Fct-Katalog Laenge: 60*/
		{
			len10 = ( (superval[currentPos]&0x7F) <<8) + superval[currentPos+1];
			currentPos = currentPos + 2;
			cnt10 = 1;
		}
		else
		{
			len10 = 60;
			writelineEx(gBAP_Trace, 0, "Anzahl der Laengeninformation-Bytes in P_FSG_80_0_0_14 ueberschreitet die erlaubte 1 Byte vom Fct-Kalatog.");
		}
	}
	else
	{
		len10 = superval[currentPos]&0x7F;
		currentPos = currentPos + 1;
		cnt10 = 0;
	}
	
	if(len10 > 60)
	{
		writelineEx(gBAP_Trace, 0, "Laenge:%d bei der Byte Position:%d ueberschreitet die Maximale erlaubte Laenge von Fct-Kalatog", len10, currentPos);
		len10 = 60;
	
		if(len10 < 128) cnt10 = 0;
		else cnt10 = 1;
	}
	
	for(i = 0; i < len10; i++)
	{
		param10[i] = superval[currentPos++];
	}
	
	putValue(P_FSG_80_0_0_14_10_len, len10);
	putValue(P_FSG_80_0_0_14_10_cnt, cnt10);
	
	if(len10 > getValueSize(P_FSG_80_0_0_14_10))
		putValue(P_FSG_80_0_0_14_10, param10, len10);
	else
		putValue(P_FSG_80_0_0_14_10, param10, getValueSize(P_FSG_80_0_0_14_10));

	///////// Start param11 - UINT8 /////////
	param11 = superval[currentPos++];
	putValue(P_FSG_80_0_0_14_11, round((param11 * 1.0) - (0) + (0), 1.0));
	///////// Start param12 - UINT8 /////////
	param12 = superval[currentPos++];
	putValue(P_FSG_80_0_0_14_12, round((param12 * 1.0) - (0) + (0), 1.0));
}

