        putValue(A_FSG_80_0_0_24_Shift, 1); //Shift = Bit 4 von Byte 0
    else
        putValue(A_FSG_80_0_0_24_Shift, 0); //Shift = Bit 4 von Byte 0
    if ( (0x20 & temp[0]) >0)
        putValue(A_FSG_80_0_0_24_Direction, 1); //Direction = Bit 5 von Byte 0
    else
        putValue(A_FSG_80_0_0_24_Direction, 0); //Direction = Bit 5 von Byte 0
    if ( (0x40 & temp[0]) >0)
        putValue(A_FSG_80_0_0_24_TransPos, 1); //Transmission Pos = Bit 6 von Byte 0
    else
        putValue(A_FSG_80_0_0_24_TransPos, 0); //Transmission Pos = Bit 6 von Byte 0
    if ( (0x80 & temp[0]) >0)
        putValue(A_FSG_80_0_0_24_IndexSize, 1); //IndexSize = Bit 7 von Byte 0
    else
        putValue(A_FSG_80_0_0_24_IndexSize, 0); //IndexSize = Bit 7 von Byte 0
    if(getValue(A_FSG_80_0_0_24_IndexSize))
    {
        putValue(A_FSG_80_0_0_24_Start, ((temp[2]<<8)| temp[1]) ); // Array Start index, 2 Byte.
        putValue(A_FSG_80_0_0_24_Elements, ((temp[4]<<8)| temp[3]) ); // Array element count, 2 Byte.
    }
    else
    {
        putValue(A_FSG_80_0_0_24_Start, temp[1]); // Array Start index, 1 Byte.
        putValue(A_FSG_80_0_0_24_Elements, temp[2]); // Array element count, 1 Byte.
    }
