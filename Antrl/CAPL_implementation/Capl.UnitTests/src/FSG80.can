        // 5 Byte Header
        head[0] = head[0] | 0x80;
        head[1] =  getValue(A_FSG_80_0_0_28_Changed_Start) & 0xFF;
        head[2] = (getValue(A_FSG_80_0_0_28_Changed_Start) >> 8 ) & 0xFF;
        head[3] =  getValue(A_FSG_80_0_0_28_Changed_Elements) & 0xFF;
        head[4] = (getValue(A_FSG_80_0_0_28_Changed_Elements)  >> 8 ) & 0xFF;
        //ToDo: DATA - derzeit einfach laenge = 5
        putValue(A_FSG_80_0_0_28_Changed, head, 5); 
    }
    else 
    {
        // 3 Byte Header
        head[0] = head[0] & 0x7F;
        head[1] = getValue(A_FSG_80_0_0_28_Changed_Start) & 0xFF;
        head[2] = getValue(A_FSG_80_0_0_28_Changed_Elements) & 0xFF;
        //ToDo: DATA - derzeit einfach laenge = 3
        putValue(A_FSG_80_0_0_28_Changed, head, 3); 
    }
}

