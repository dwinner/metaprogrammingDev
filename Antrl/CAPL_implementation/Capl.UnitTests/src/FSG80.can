A_FSG_80_0_0_28_GenerateStatus()
{
    byte head[5];
    int i = 0;
    int headLength;
    long tempValue;
    int currentPos;
    //Record Address in Byte 0 Bit 0-3 kopieren - MAX Wert = 0x0F
    if(getValue(A_FSG_80_0_0_28_RecAdr) > 0x0F)
        putValue(A_FSG_80_0_0_28_RecAdr , 0x0F);
    head[0] = (head[0] & 0xF0) | getValue(A_FSG_80_0_0_28_RecAdr);
    //Shift Bit in Byte 0 Bit 4 kopieren
    if( getValue(A_FSG_80_0_0_28_Shift) > 0) head[0] = head[0] | 0x10;
    else head[0] = head[0] & 0xEF;
    // Direction Bit in Byte 0 Bit 5 kopieren
    if( getValue(A_FSG_80_0_0_28_Direction) > 0) head[0] = head[0] | 0x20;
    else head[0] = head[0] & 0xDF;
    // TransmissionPos Bit in Byte 0 Bit 6 kopieren
    if( getValue(A_FSG_80_0_0_28_TransPos) > 0) head[0] = head[0] | 0x40;
    else head[0] = head[0] & 0xBF;
    if( getValue(A_FSG_80_0_0_28_IndexSize) > 0)
    {
        // 5 Byte Header
        head[0] = head[0] | 0x80;
        head[1] =  getValue(A_FSG_80_0_0_28_Start) & 0xFF;
        head[2] = (getValue(A_FSG_80_0_0_28_Start) >> 8 ) & 0xFF;
        head[3] =  getValue(A_FSG_80_0_0_28_Elements) & 0xFF;
        head[4] = (getValue(A_FSG_80_0_0_28_Elements)  >> 8 ) & 0xFF;
        //ToDo: DATA - derzeit einfach laenge = 5
        headLength = 5;
    }
    else 
    {
        // 3 Byte Header
        head[0] = head[0] & 0x7F;
        head[1] = getValue(A_FSG_80_0_0_28_Start) & 0xFF;
        head[2] = getValue(A_FSG_80_0_0_28_Elements) & 0xFF;
        //ToDo: DATA - derzeit einfach laenge = 3
        headLength = 3;
    }

    currentPos = 0;

    putValue(A_FSG_80_0_0_28, head, headLength); 
