	byte param2; // DownloadState
	byte param3; // AvailableCommandTypes
	byte param4[362]; // ExceptionText
	word len4;
	byte cnt4;
	byte param5; // Extension2
	byte param6; // Extension3

	int i;
	byte superval[366];
