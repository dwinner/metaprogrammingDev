    _mIC_DriveAssistSys_AR.byte( 7 ) = this.byte( 7 );

//************************************************************************************************

/*    if( !getValue( HMI_SQC_RollTestMd_SPCR_Stat_IC_Auto ))
    {
        // berechne message counter
        if( _mIC_DriveAssistSys_AR.SQC_RollTestMd_SPCR_Stat_IC >= 15 ) _mIC_DriveAssistSys_AR.SQC_RollTestMd_SPCR_Stat_IC = 0;
        else _mIC_DriveAssistSys_AR.SQC_RollTestMd_SPCR_Stat_IC++;
