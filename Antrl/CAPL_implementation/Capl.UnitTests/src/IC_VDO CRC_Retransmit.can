    }
    else
        _mIC_DriveAssistSys_AR.SQC_RollTestMd_SPCR_Stat_IC = getValue( HMI_SQC_RollTestMd_SPCR_Stat_IC );
*/
    if( !getValue( HMI_CRC_RollTestMd_SPCR_Stat_IC_Auto ) )
    {   // CRC according Profile_02 ( W213 )
        // berechne crc
        crc = 0xFF;
//      crc = crctable_profile02[ crc ^ _mIC_DriveAssistSys_AR.byte( 0 ) ]; // CRC
        crc = crctable_profile02[ crc ^ _mIC_DriveAssistSys_AR.byte( 1 ) ];
        crc = crctable_profile02[ crc ^ _mIC_DriveAssistSys_AR.byte( 2 ) ];
        crc = crctable_profile02[ crc ^ _mIC_DriveAssistSys_AR.byte( 3 ) ];
//      crc = crctable_profile02[ crc ^ _mIC_DriveAssistSys_AR.byte( 4 ) ];
