        crc = crctable_profile02[ crc ^ _IC_DriveAssistSys_AR_CRC_RollTestMd_SPCR_Stat_IC[ _mIC_DriveAssistSys_AR.SQC_RollTestMd_SPCR_Stat_IC ] ]; // load offset
