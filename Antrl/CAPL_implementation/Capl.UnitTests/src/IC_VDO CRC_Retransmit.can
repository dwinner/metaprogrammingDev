    putValue( HMI_IC_Err_Stat,   ( this.IC_Err_Stat ) );
    putValue( HMI_IC_Func_Stat,   ( this.IC_Func_Stat ) );
    putValue( HMI_Park_IconDisp_Rs,   ( this.Park_IconDisp_Rs ) );
    putValue( HMI_Park_IconDisp_Stat_IC,   ( this.Park_IconDisp_Stat_IC ) );
    putValue( HMI_Park_Warn1_Disp_Rs,   ( this.Park_Warn1_Disp_Rs ) );
    putValue( HMI_Park_Warn1_Disp_Stat_IC,   ( this.Park_Warn1_Disp_Stat_IC ) );
    putValue( HMI_Rsrv0_Park_Disp_Rs,   ( this.Rsrv0_Park_Disp_Rs ) );
    putValue( HMI_Rsrv1_Park_Disp_Rs,   ( this.Rsrv1_Park_Disp_Rs ) );
    putValue( HMI_Rsrv2_Park_Disp_Rs,   ( this.Rsrv2_Park_Disp_Rs ) );
    putValue( HMI_Rsrv3_Park_Disp_Rs,   ( this.Rsrv3_Park_Disp_Rs ) );
    putValue( HMI_Rsrv4_Park_Disp_Rs,   ( this.Rsrv4_Park_Disp_Rs ) );
    putValue( HMI_Rsrv5_Park_Disp_Rs,   ( this.Rsrv5_Park_Disp_Rs ) );
    putValue( HMI_Rsrv6_Park_Disp_Rs,   ( this.Rsrv6_Park_Disp_Rs ) );
    putValue( HMI_Rsrv7_Park_Disp_Rs,   ( this.Rsrv7_Park_Disp_Rs ) );
    putValue( HMI_SQC_Park_Disp_Rs,   ( this.SQC_Park_Disp_Rs ) );

    _mPark_Disp_Rs_AR.byte( 1 ) = this.byte( 1 );
    _mPark_Disp_Rs_AR.byte( 2 ) = this.byte( 2 );
    _mPark_Disp_Rs_AR.byte( 3 ) = this.byte( 3 );
    _mPark_Disp_Rs_AR.byte( 4 ) = this.byte( 4 );
    _mPark_Disp_Rs_AR.byte( 5 ) = this.byte( 5 );
    _mPark_Disp_Rs_AR.byte( 6 ) = this.byte( 6 );
    _mPark_Disp_Rs_AR.byte( 7 ) = this.byte( 7 );

//************************************************************************************************

/*    if( !getValue( HMI_SQC_RollTestMd_SPCR_Stat_IC_Auto ))
    {
        // berechne message counter
        if( _mIC_DriveAssistSys_AR.SQC_RollTestMd_SPCR_Stat_IC >= 15 ) _mIC_DriveAssistSys_AR.SQC_RollTestMd_SPCR_Stat_IC = 0;
        else _mIC_DriveAssistSys_AR.SQC_RollTestMd_SPCR_Stat_IC++;
        putValue(HMI_SQC_RollTestMd_SPCR_Stat_IC, _mIC_DriveAssistSys_AR.SQC_RollTestMd_SPCR_Stat_IC );
    }
    else
        _mIC_DriveAssistSys_AR.SQC_RollTestMd_SPCR_Stat_IC = getValue( HMI_SQC_RollTestMd_SPCR_Stat_IC );
*/
    if( !getValue( HMI_CRC_Park_Disp_Rs_Auto ) )
    {   // CRC according Profile_02 ( W213 )
        // berechne crc
        crc = 0xFF;
//      crc = crctable_profile02[ crc ^ _mPark_Disp_Rs_AR.byte( 0 ) ]; // CRC
        crc = crctable_profile02[ crc ^ _mPark_Disp_Rs_AR.byte( 1 ) ];
        crc = crctable_profile02[ crc ^ _mPark_Disp_Rs_AR.byte( 2 ) ];
        crc = crctable_profile02[ crc ^ _mPark_Disp_Rs_AR.byte( 3 ) ];
        crc = crctable_profile02[ crc ^ _mPark_Disp_Rs_AR.byte( 4 ) ];
        crc = crctable_profile02[ crc ^ _mPark_Disp_Rs_AR.byte( 5 ) ];
        crc = crctable_profile02[ crc ^ _mPark_Disp_Rs_AR.byte( 6 ) ];
        crc = crctable_profile02[ crc ^ _mPark_Disp_Rs_AR.byte( 7 ) ];
        crc = crctable_profile02[ crc ^ _Park_Disp_Rs_AR_CRC_Park_Disp_Rs[ _mPark_Disp_Rs_AR.SQC_Park_Disp_Rs ] ]; // load offset
        crc = (crc ^ 0xFF);
        if( getValue(  HMI_CRC_Park_Disp_Rs_Invalid ) )
            crc++;
        _mPark_Disp_Rs_AR.CRC_Park_Disp_Rs = crc;
        putValue( HMI_CRC_Park_Disp_Rs, crc );
    }
    else
        _mPark_Disp_Rs_AR.CRC_Park_Disp_Rs = getValue( HMI_CRC_Park_Disp_Rs );

    _mPark_Disp_Rs_AR.ID = 0x18C;
    output( _mPark_Disp_Rs_AR );
    if( this.CRC_Park_Disp_Rs.phys == _mPark_Disp_Rs_AR.CRC_Park_Disp_Rs )
        putvalue( Env_CRC_Check__Park_Disp_Rs_AR, 0 );
    else
        putvalue( Env_CRC_Check__Park_Disp_Rs_AR, 1 );


//*****************************************************************************************************
// --- cyclicAndSpontanWithDelay 5 --- 200ms cycle time ---
// Evaluate if received message is on event 
    bAliveCtr = getValue( HMI__MSGAliveCtr_Park_Disp_Rs_AR );
    if( _Park_Disp_Rs_AR_OnEventTrap_Active )
    {
        putValue( HMI__MSG_OnEvent_Park_Disp_Rs_AR, 1 );
        cancelTimer( tPark_Disp_Rs_AR_OnEventIND );
        setTimer( tPark_Disp_Rs_AR_OnEventIND, 1000 );
    }
    else
    {
        _Park_Disp_Rs_AR_CycleReduction++;
        if( _Park_Disp_Rs_AR_CycleReduction >= _cPark_Disp_Rs_AR_CycleReductionValue )
        {
            _Park_Disp_Rs_AR_CycleReduction = 0;
            if( bAliveCtr < 8 )
            {
                bAliveCtr = ( bAliveCtr + 1 ) & 0x07;
            }
            else
            {
                bAliveCtr = 0;
            }
        }
        putValue( HMI__MSGAliveCtr_Park_Disp_Rs_AR, bAliveCtr );
        setTimer( tPark_Disp_Rs_AR_OnEventTrap, _Park_Disp_Rs_AR_OnEventDetectTime );
    }
}
/*@@end*/

