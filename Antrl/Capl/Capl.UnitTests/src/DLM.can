/*@@caplFunc:ROE_DLM_check_output():*/
void ROE_DLM_check_output()
{
    if (!getValue( HMI__STMSG_ROE_DLM))
    {
        // Botschaft ROE_DLM --- Sendeart 9 cyclicWithRepeatOnDemand --- 
        if( _ROE_DLM_Cnt > 0 )
        {
            _ROE_DLM_Cnt--;
            if( _ROE_DLM_Cnt < 1 )
            {
                // Nachricht wg. abgelaufenem Zyklus ausgeben
                ROE_DLM_output();
                _ROE_DLM_MsgRepetitions--;
               if( _ROE_DLM_MsgRepetitions > 0 ) _ROE_DLM_Cnt = _ROE_DLM_CycleCount;
            }
        }
    }
}
/*@@end*/

/*@@caplFunc:ROE_DLM_init():*/
ROE_DLM_init()
{   /* Botschaft ROE_DLM ID: 2565810944 --- Sendeart 9 cyclicWithRepeatOnDemand --- 
BA_ "ApplMessage" BO_ 2565810944 0;
BA_ "NmAsrMessage" BO_ 2565810944 0;
BA_ "GenMsgMinAcceptLength" BO_ 2565810944 8;
BA_ "GenMsgILSupport" BO_ 2565810944 1;
BA_ "DiagState" BO_ 2565810944 0;
BA_ "DiagRequest" BO_ 2565810944 0;
BA_ "DiagResponse" BO_ 2565810944 0;
BA_ "GenMsgStartValue" BO_ 2565810944 "0";
BA_ "GenMsgDelayTime" BO_ 2565810944 5;
BA_ "GenMsgNrOfRepetitions" BO_ 2565810944 0;
BA_ "GenMsgCycleTime" BO_ 2565810944 200;
BA_ "GenMsgSendType" BO_ 2565810944 9;
    */
    _ROE_DLM_Timebase =         c_tDBKomTimer_20ms;
    _ROE_DLM_CycleCount =     200 / _ROE_DLM_Timebase;
    _ROE_DLM_StartDelay =       5 / _ROE_DLM_Timebase;
    _ROE_DLM_Cnt = _ROE_DLM_StartDelay;
    _ROE_DLM_MsgRepetitions = 0;

    putValue( HMI__STMSGSD_ROE_DLM, _ROE_DLM_StartDelay * _ROE_DLM_Timebase);

    _mROE_DLM.ROE_DTC_DLM.phys = getValue( HMI_ROE_DTC_DLM );
    _mROE_DLM.ROE_DTC_Stat_DLM.phys = getValue( HMI_ROE_DTC_Stat_DLM );
    _mROE_DLM.ROE_Header_DLM.phys = getValue( HMI_ROE_Header_DLM );
}
/*@@end*/

/*@@caplFunc:ROE_DLM_output():*/
ROE_DLM_output(void)
{
    output( _mROE_DLM );
}
/*@@end*/

/*@@caplFunc:ROE_DLM_restart(int):*/
void ROE_DLM_restart(int nNumRepetitions)
{
    if (nNumRepetitions > _ROE_DLM_MsgRepetitions)
    {
        if (_ROE_DLM_MsgRepetitions < 1)
            _ROE_DLM_Cnt = 1;
        _ROE_DLM_MsgRepetitions = nNumRepetitions;
    }
}
/*@@end*/

