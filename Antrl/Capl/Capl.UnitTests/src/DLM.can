/*@@startStart:Start:*/
on start
{
    putValue( HMI__NM_DLM_NM_Request, 0 );  // reset all requests
    _NM_Switch_NM_Mode();
    
    __Init_Msg();
    __Startup_Delay();
    __Reset_E2E_SQC();

    putValue( HMI__NM_DLM_AutomaticModeActive, 0 );
    putValue( HMI__NM_DLM_AutomaticModeActive, 1 );
}
/*@@end*/

/*@@caplFunc:__Init_Msg():*/
__Init_Msg()
{
// #START_INIT#
    DLM_Stat_AR_init();
    ROE_DLM_init();
// #START_INIT_END#
//    ECU_APPL_DLM_1_init();
//    ECU_APPL_DLM_2_init();
//    Meas_DLM_1_init();
    NM_xxx_AR_init();
}
/*@@end*/

/*@@envVar:HMI__STOP_All_MESSAGES_DEFAULT_STARTDELAY:*/
on envVar HMI__STOP_All_MESSAGES_DEFAULT_STARTDELAY
{
    if( getValue( this ) )
    {
        __Init_Msg();
        __Startup_Delay();
    }
}
/*@@end*/

/*@@envVar:HMI__STOP_All_MESSAGES_SET_STARTDELAY:*/
on envVar HMI__STOP_All_MESSAGES_SET_STARTDELAY
{
    if( getValue( this ) )
    {
        __Startup_Delay();
    }
}
/*@@end*/

/*@@caplFunc:__Startup_Delay():*/
__Startup_Delay()
{
//    write(" Delay setzen :__DLM_Startup_Delay");
// #START_DELAY#
    _DLM_Stat_AR_Cnt =   getValue( HMI__STMSGSD_DLM_Stat_AR   ) / _DLM_Stat_AR_Timebase;
    _ROE_DLM_Cnt =              getValue( HMI__STMSGSD_ROE_DLM       )        / _ROE_DLM_Timebase;
// #START_DELAY_END#
}
/*@@end*/

/*@@caplFunc:__Reset_E2E_SQC():*/
__Reset_E2E_SQC()
{
    putValue( HMI_SQC_DLM_Stat, 15 );
//    write(" E2E-simu: Reset SQC to 15 ");
}
/*@@end*/

/*@@timer:t_DBKomTimer_5ms:*/
on Timer t_DBKomTimer_5ms
{
// *** DBKom-Sendefunktion ***
    // timer aufziehen für nächsten Zyklus
    if( getValue( HMI__NM_DLM_CommActive ) |
        getValue( HMI__NM_DLM_RequestBusCom )   )
    {
        setTimer( t_DBKomTimer_5ms, c_tDBKomTimer_5ms );
    }
// #CYC_TIMER_5_MS#
    DLM_Stat_AR_check_output();                   // Botschaft CLkS_Rq_AR                   --- Sendeart 8 cyclicIfActiveFast --- 
// #CYC_TIMER_5_MS_END#
}
/*@@end*/

/*@@timer:t_DBKomTimer_20ms:*/
on Timer t_DBKomTimer_20ms
{
// *** DBKom-Sendefunktion ***
    // timer aufziehen für nächsten Zyklus
    if( getValue( HMI__NM_DLM_CommActive ) |
        getValue( HMI__NM_DLM_RequestBusCom )   )
    {
        setTimer( t_DBKomTimer_20ms, c_tDBKomTimer_20ms );
    }
// #CYC_TIMER_20_MS#
// 5ms    DLM_Stat_AR_check_output();
    ROE_DLM_check_output();
// #CYC_TIMER_20_MS_END#
//    NM_DLM_Stat_AR_AR_check_output();
}
/*@@end*/

/*@@envVar:HMI__SAVE_PANEL_CONFIG_DLM:*/
on EnvVar HMI__SAVE_PANEL_CONFIG_DLM
{
    dword hFile;

    if( getValue(this) )
    {
        write("Saving HMI Panel Variables ...");

        hFile = openFileWrite( "CONFIG\\HMI_PRESETS.ASC", 0 );
        filePutString( "base hex timestamps absolute\n", 31, hFile );

// #MSG_SAVEENVVAR#
        DLM_Stat_AR_saveEnvVars( hFile );
        ROE_DLM_saveEnvVars( hFile );
// #MSG_SAVEENVVAR_END#
 //       NM_DLM_Stat_AR_AR_saveEnvVars( hFile );
        NM_xxx_AR_saveEnvVars( hFile );

        fileClose(hFile );
    }
}
/*@@end*/

