Split(char bufin[], char delimiter, char bufout1[], char bufout2[])
{
  dword i,j;
  byte found;
  
  i=j=0;
  found = 0;
  while(bufin[i] != '\0' &&
        bufin[i] != '\r' &&
        bufin[i] != '\n')    
  {
    if(found == 0)
    {  
      if(bufin[i] != delimiter)
      {
        bufout1[i] = bufin[i];
      }
      else
      {
        found = 1;
        bufout1[i] = '\0';
      }
    }
    else
    {
      bufout2[j] = bufin[i];
      j++;
    }
    i++;
  }
  bufout1[i] = '\0';
  bufout2[j] = '\0';
}

