		if(len3 < 128) cnt3 = 0;
		else cnt3 = 1;
	}
	
	for(i = 0; i < len3; i++)
	{
		param3[i] = superval[currentPos++];
	}
	
	putValue(P_FSG_42_0_0_16_3_len, len3);
	putValue(P_FSG_42_0_0_16_3_cnt, cnt3);
	
	if(len3 > getValueSize(P_FSG_42_0_0_16_3))
		putValue(P_FSG_42_0_0_16_3, param3, len3);
	else
		putValue(P_FSG_42_0_0_16_3, param3, getValueSize(P_FSG_42_0_0_16_3));

