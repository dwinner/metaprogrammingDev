// Updates the bus value field and other dependencies.
P_FSG_42_0_0_16_BuildFct()
{
	byte param1; // TrafficLight
	byte param2; // MainDirection
	byte param3[18]; // Sidestreets
	word len3;
	byte cnt3;
	byte param4; // TrafficLightWarnings
	byte param5; // TrafficLightLayout

	int i;
	byte superval[22];
	int currentPos;
	currentPos = 0;
// gen_BuildFunc_SetSuperVals 
	param1 = getValue(P_FSG_42_0_0_16_1);
	superval[currentPos++]=param1;
// gen_BuildFunc_SetSuperVals 
	@P_FSG_42_0_0_16_2_range = round(@P_FSG_42_0_0_16_2 + (0), 1.0); // slider value + min = real value
	param2 = round( (@P_FSG_42_0_0_16_2 + (0) - (0)) / (1.0), 1.0); // (slider value + min - offset) / step = busvalue
	superval[currentPos++]=param2;
// gen_BuildFunc_SetSuperVals 
	getValue(P_FSG_42_0_0_16_3, param3);
	len3 = getValue(P_FSG_42_0_0_16_3_len);
	cnt3= getValue(P_FSG_42_0_0_16_3_cnt);
	
	/* check box aktiviert*/
	if (cnt3 == 1)
	{
		/* 0 bis 3 (MaxString - 1B oder 2B) */
		if(len3 >= 0 && len3 <= 17)
		{
			//writelineEx(gBAP_Trace, 0, "[Debug-1] Len3: %d", len3);
			superval[currentPos] = (len3&0x7f00) | 0x80;
			superval[currentPos+1] = len3 & 0x00ff;
		}
		else
		{
			writelineEx(gBAP_Trace, 0, "Eingegebene Laenge: %d von P_FSG_42_0_0_16_3 ist ungueltig. Die max. erlaubte Laenge ist %d.", len3, 3);
			return;
		}
	}
	else if(cnt3 == 0) /*check box deaktiviert*/
	{
		if(len3 >= 0 && len3 <= 127)
		{
			//writelineEx(gBAP_Trace, 0, "[Debug-1] Len3: %d", len3);
			superval[currentPos]= len3 & 0x00ff;
			putValue(P_FSG_42_0_0_16_3_len, len3);
		}
		else if(len3 > 127 && len3 <= 17)
		{
			superval[currentPos] = (len3&0x7f00) | 0x80;
			superval[currentPos+1] = len3 & 0x00ff;
			putValue(P_FSG_42_0_0_16_3_len, len3);
			putValue(P_FSG_42_0_0_16_3_cnt, 1);
		}
		else
		{
			writelineEx(gBAP_Trace, 0, "Eingegebene Laenge: %d von P_FSG_42_0_0_16_3 ist ungueltig. Die max. erlaubte Laenge ist %d.", len3, 3);
			return;
		}
	}
	currentPos += cnt3+1;
	for(i = 0; i < len3; i++)
	{
		superval[currentPos++]= param3[i];
	}

// gen_BuildFunc_SetSuperVals 
	param4 = getValue(P_FSG_42_0_0_16_4);
	@P_FSG_42_0_0_16_4_0 = (param4 & 1) > 0;
	@P_FSG_42_0_0_16_4_1 = (param4 & 2) > 0;
	superval[currentPos++]=param4;
// gen_BuildFunc_SetSuperVals 
	param5 = getValue(P_FSG_42_0_0_16_5);
	superval[currentPos++]=param5;
	putValue(P_FSG_42_0_0_16, superval, currentPos);
}


