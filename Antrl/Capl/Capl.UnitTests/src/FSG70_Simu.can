	switch(type){
		case 0:
			EnableCANFrame(1);
			EnableFRFrame(0);
			EnableEthernetFrame(0);
			writeLineEx(0,1,"CAN Selected");
			break;
		case 1:
			EnableCANFrame(0);
			EnableFRFrame(1);
			EnableEthernetFrame(0);
			writeLineEx(0,1,"FR Selected");
			break;
		case 2:
			EnableCANFrame(0);
			EnableFRFrame(0);
			EnableEthernetFrame(1);
			writeLineEx(0,1,"ETHERNET Selected");
			break;
		default:
			writeLineEx(0,3, "!!BUS TYPE ERROR!!");    
			break;
	}
